{
    "name": "flashytime/db-cache",
    "description": "A PHP library to cache database, supports MySQL,Mongo and Memcached,Redis...",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "flashytime",
            "email": "myflashytime@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Flashytime\\DbCache\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Flashytime\\DbCache\\Tests\\": "tests/"
        }
    }
}
